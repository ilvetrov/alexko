<div class="select-pop-up {{ selectData.modClass }} pop-up hidden disabled" data-pop-up="{{ selectData.name }}" data-pop-up-event="{{ selectData.event }}" data-pop-up-set-position-to-button>
  <div class="select-pop-up__content" data-pop-up-content>
    {{#each selectData.items as |item|}}
      <div class="select-pop-up__item-wrap">
        <{{#if item.link}}a href="{{ item.link }}"{{else}}div{{/if}} class="select-pop-up__item click-extender {{iif item.active 'active' ''}}" data-option-id="{{ item.id }}" data-event-button>
          {{ item.content }}
        </{{#if item.link}}a{{else}}div{{/if}}>
        <!-- /.select-pop-up__item -->
      </div>
      <!-- /.select-pop-up__item-wrap -->
    {{/each}}
  </div>
  <!-- /.select-pop-up__content -->
</div>
<!-- /.select-pop-up -->